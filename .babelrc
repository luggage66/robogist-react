{
    "presets": [ "es2015" ],
    "plugins": [
        "transform-react-jsx",
        "transform-object-rest-spread",
        "transform-runtime",
        "transform-decorators-legacy",
        "transform-class-properties",
        "syntax-async-functions",
        "transform-regenerator",
        ["import", {
            "libraryName": "antd",
            "style": "css"
        }]
    ]
}
